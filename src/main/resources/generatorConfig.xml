<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>

    <context id="venue" targetRuntime="Mybatis3">
        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/s328"
                        userId="root" password="123456">
        </jdbcConnection>
        <!--存放生成实体类的路径-->
        <javaModelGenerator targetPackage="com.woniu.yoga.user.pojo" targetProject="src/main/java"/>
        <!--生成sql映射文件的路径-->
        <sqlMapGenerator targetPackage="mapper" targetProject="src/main/resources/userMapper"/>
        <!--生成dao接口的目录-->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.woniu.yoga.user.dao" targetProject="src/main/java"/>

        <table tableName="course" mapperName="CourseMapper" domainObjectName="Course" enableDeleteByExample="false"
               enableUpdateByExample="false"
               enableSelectByExample="false"
               enableCountByExample="false"
               selectByExampleQueryId="false"></table>
    </context>


</generatorConfiguration>